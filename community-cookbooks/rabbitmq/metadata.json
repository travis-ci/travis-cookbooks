    "rabbitmq/nodename": {
      "display_name": "RabbitMQ Erlang node name",
      "description": "The Erlang node name for this server.",
      "default": "node['hostname']"
    },
    "rabbitmq/address": {
      "display_name": "RabbitMQ server IP address",
